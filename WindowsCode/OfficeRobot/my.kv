<MyGrid>:
    img: img
    button: button

    BoxLayout:

        padding:5

        Image:
            id: img
            source: 'officeLayout.PNG'

            # Giving the size of image
            size_hint_x: 2

            # allow stretching of image
            allow_stretch: True

            Button:
                id: button
                text: "Home" if button.state =="normal" else "Other"
                background_color: 0.1, 0.5, 0.6, 1
                size_hint: 0.1, 0.1
                pos: 100,100
                on_press:root.press()

